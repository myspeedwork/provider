{
    "name": "speedwork/provider",
    "type": "library",
    "description": "Speedwork providers",
    "license": "MIT",
    "authors": [{
        "name": "sankar",
        "email": "sankar.suda@gmail.com"
    }],
    "require": {
        "php": ">=5.6"
    },
    "replace" : {
        "speedwork/container" : "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "satooshi/php-coveralls": "2.*"
    },
    "suggest": {
        "symfony/http-foundation": "^3.1 for session support",
        "monolog/monolog": ">=1.0.0 for logging",
        "filp/whoops": "^2.1 for php error logging"
    },
    "autoload": {
        "psr-4": {
            "Speedwork\\Provider\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
